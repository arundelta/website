          <!--/stories-->
          <h3 style="margin-left:20%;">Create an account</h3>

          <div style="margin-left:20%;" >
            <form class="col-md-6">
              
             <div class="form-group" >
                <label for="exampleInputname">Name</label>
                <input type="name" class="form-control" id="exampleInputname1" placeholder="Enter Your Name" >
              </div>
              <div class="form-group">
                <label for="exampleInputmobile">Mobile</label>
                <input type="name" class="form-control" id="exampleInputmobile1" placeholder="Enter Mobile Number" required >
                <small id="emailHelp" class="form-text text-muted">You can use following formats.<li>&nbsp;a) XXX-XXX-XXXX</li>
        
                  <li>&nbsp;b) XXX.XXX.XXXX&nbsp;</li>
                  <li>&nbsp;c) XXX XXX XXXX</li></small>
              </div>
          <div class="form-group">
            <label for="exampleInputEmail1">Email address</label>
            <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" required >
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
             
              <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" minlength="8" required pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{6,}">
              <small id="emailHelp" class="form-text text-muted">At least 8 characters - uppercase and lowercase letters and a number.
                Inclusion of at least one special character, e.g., ! @ # ? ].</small>
              <meter min="0" low="4" optimum="8" high="10" max="100" value="">
           </meter>
          </div>
          <div class="form-group">
            <label for="exampleInputPassword2">ReType Password</label>
            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" minlength="8" >
          </div>
          <div class="date">
            <label for="dob">Date of Birth</label>
            <input type="date" class="form-control" id="dob" >
          </div>
          <br><br>
          

          <button type="submit" class="btn btn-primary" onclick="phonenumber();checkpwd()">Submit </button>
        </form>
        </div>
<br>
